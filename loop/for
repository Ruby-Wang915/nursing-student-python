#while 結構
k=0
while k<10:
    print(k)  
    k+=1

print()

#for 結構
#for k in range(intial,end,step)
for k in range(10):  #initialValue=0/endValue=10-1
    print(k)

print()

for n in range(1,10): #initialValue=1/endValue=10-1
    print(n)  

print()

for x in range(1,20,3):
    print(x)

print()

for y in range(10,0,-2):
    print(y)

print()

for z in 'Ruby':  #也可印str 但range裡只放int 所以不加
    print(z)
